{
  "name": "ultra-gear-cms",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "generate:dev": "dotenv -e backend/.env -- prisma generate --schema=backend/prisma/schema.prisma",
    "migrate:dev": "dotenv -e backend/.env -- prisma migrate dev --schema=backend/prisma/schema.prisma --name add_new_models",
    "generate:test": "dotenv -e backend/.env.test -- prisma generate --schema=backend/prisma/schema.prisma",
    "migrate:test": "dotenv -e backend/.env.test -- prisma migrate dev --schema=backend/prisma/schema.prisma --name init",
    "initialize": "node initialize.js",
    "test:backend": "dotenv -e backend/.env.test -- jest --config backend/jest.config.js",
    "studio:dev": "dotenv -e backend/.env -- prisma studio --schema=backend/prisma/schema.prisma",
    "studio:test": "dotenv -e backend/.env.test -- prisma studio --schema=backend/prisma/schema.prisma"
  },
  "prisma": {
    "schema": "./backend/prisma/schema.prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "apollo-server": "^3.13.0",
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "graphql": "^16.9.0",
    "graphql-upload": "^16.0.2",
    "prisma": "^5.17.0"
  },
  "devDependencies": {
    "dotenv-cli": "^7.4.2",
    "jest": "^29.7.0",
    "supertest": "^7.0.0"
  }
}
