{
  "name": "ultra-gear-cms",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "generate:dev": "dotenv -e backend/.env -- prisma generate --schema=backend/prisma/schema.prisma",
    "migrate:dev": "dotenv -e backend/.env -- prisma migrate dev --schema=backend/prisma/schema.prisma --name add-archiving-to-reviews",
    "generate:test": "dotenv -e backend/.env.test -- prisma generate --schema=backend/prisma/schema.prisma",
    "migrate:test": "dotenv -e backend/.env.test -- prisma migrate dev --schema=backend/prisma/schema.prisma --name add-archiving-to-reviews",
    "initialize": "node initialize.js",
    "test:backend": "mocha --require esm --recursive test",
    "test:unit": "mocha --require esm --recursive test/unit",
    "test:integration": "dotenv -e .env.test -- mocha --require esm --recursive test/integration",
    "studio:dev": "dotenv -e backend/.env -- prisma studio --schema=backend/prisma/schema.prisma",
    "studio:test": "dotenv -e backend/.env.test -- prisma studio --schema=backend/prisma/schema.prisma"
  },
  "mocha": {
    "require": [
      "dotenv/config"
    ],
    "reporter": "spec",
    "timeout": 5000,
    "exit": true
  },
  "prisma": {
    "schema": "./backend/prisma/schema.prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "apollo-server": "^3.13.0",
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "esm": "^3.2.25",
    "express": "^4.19.2",
    "graphql": "^16.9.0",
    "graphql-upload": "^16.0.2",
    "prisma": "^5.17.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.1.0",
    "@types/supertest": "^6.0.2",
    "dotenv-cli": "^7.4.2",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
